{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Service-Worker-Allowed", "value": "/" },
        { "key": "Cross-Origin-Resource-Policy", "value": "cross-origin" }, { "key": "Cross-Origin-Embedder-Policy", "value": "credentialless", "_comment": "[Fix F1/E1]" },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
      ]
    }
  ],
  "redirects": [
    { "source": "/404", "destination": "/", "permanent": false }
  ]
}
